<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="department" class="com.ust.task.hrms_project.Department" >
        <property name="id" value="1"/>
        <property name="name" value="cse"/>
    </bean>
    
    <bean id="department2" class="com.ust.task.hrms_project.Department">
        <property name="id" value="2"/>
        <property name="name" value="AI"/>
    </bean>

    <bean id="employee" class="com.ust.task.hrms_project.Employee" autowire="byName">
        <property name="id" value="2"/>
        <property name="name" value="Raji"/>
    </bean>
</beans>
